@model IEnumerable<ToDoListWithMigrations.Models.Category>

@{
    ViewData["Title"] = "Item - Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>List of Categories</h1>

<table class="table">
    <tr>
       <th>
            @Html.DisplayNameFor(model => model.Name)
	   </th>
	</tr>
	@foreach (var listCategory in Model)
	{
    	<tr>
            <td>
                @Html.DisplayFor(modelCategory => listCategory.Name)
            </td>
        </tr>
	}
</table>

<p>@Html.ActionLink("Create new category", "Create")</p>